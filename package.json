{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "One liner node.js example app of interaction with apaleo API",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "format": "prettier --write .",
    "serve": "node dist/server.js",
    "dev-serve": "npm run clean && npm run build && npm run serve",
    "start": "node ./dist/src/server.js",
    "dev": "tsnd --respawn ./src/server.ts"
  },
  "dependencies": {
    "@azure/core-http": "^1.2.3",
    "@sendgrid/mail": "^7.4.2",
    "@types/compression": "^1.7.0",
    "@types/qrcode": "^1.4.0",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.5.0",
    "helmet": "^4.4.1",
    "qrcode": "^1.4.4",
    "twilio": "^3.59.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.33",
    "copyfiles": "^2.4.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.19.0",
    "html-loader": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.17",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.3",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "license": "MIT"
}
